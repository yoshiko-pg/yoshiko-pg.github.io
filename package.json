    "react-pdf": "^10.2.0",
    "react-router-dom": "^7.10.1",
    "react-window": "^2.2.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@types/ejs": "^3.1.5",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/react-window": "^2.0.0",
